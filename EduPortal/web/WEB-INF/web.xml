<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee         http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>FileUploadServletExample</display-name>
    <servlet>
        <display-name>FileUploadServlet</display-name>
        <servlet-name>FileUploadServlet</servlet-name>
        <servlet-class>net.codejava.upload.FileUploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileUploaderServletDemo</servlet-name>
        <servlet-class>net.codejava.upload.FileUploaderServletDemo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SessioSetAttribute</servlet-name>
        <servlet-class>net.codejava.upload.SessioSetAttribute</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileUploaderServletNotes</servlet-name>
        <servlet-class>net.codejava.upload.FileUploaderServletNotes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SessionAttributeAdmin</servlet-name>
        <servlet-class>net.codejava.upload.SessionAttributeAdmin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FileUploadServlet</servlet-name>
        <url-pattern>/uploadFiles</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileUploaderServletDemo</servlet-name>
        <url-pattern>/testedFile1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SessioSetAttribute</servlet-name>
        <url-pattern>/SessioSetAttribute</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileUploaderServletNotes</servlet-name>
        <url-pattern>/FileUploaderServletNotes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SessionAttributeAdmin</servlet-name>
        <url-pattern>/SessionAttributeAdmin</url-pattern>
    </servlet-mapping>
</web-app>
